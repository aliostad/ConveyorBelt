    <Imports>
    </Imports>
    <ConfigurationSettings>
      <Setting name="ConveyorBelt.ElasticSearchUrl" />
      <Setting name="ConveyorBelt.TableName" />
      <Setting name="ConveyorBelt.FrequencyInSeconds" />
